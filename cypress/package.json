{
  "name": "saucedemo",
  "version": "1.0.0",
  "description": "basic example how to run Cypress tests",
  "main": "index.js",
  "scripts": {
    "test": "cypress run",
    "prepare": "cd .. && husky install cypress/.husky",
    "-------- cypress --------": "",
    "cy:open": "cypress open --e2e --browser=chrome",
    "cy:run_chrome": "cypress run --e2e --browser=chrome",
    "cy:run": "cypress run --e2e",
    "cy:run_edge": "cypress run --e2e --browser=edge",
    "cy:run_firefox": "cypress run --e2e --browser=firefox"
  },
  "private": true,
  "devDependencies": {
    "cypress": "13.1.0"
  },
  "dependencies": {
    "husky": "^8.0.3"
  }
}
